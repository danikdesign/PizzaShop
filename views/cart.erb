<h2>Your cart</h2>
<table class="table table-striped">
  <thead>
  <tr>
    <th scope="col">Pizza</th>
    <th scope="col">Quantity</th>
  </tr>
  </thead>
  <tbody>
    <% @items.each do |item| %>
      <tr>
        <td><%= item[0] %></td>
        <td><%= item[1] %></td>
      </tr>
    <% end %>
  </tbody>
</table>